@model Dictionary<string, LdapDnsWebApp.Models.GroupedZoneSummary>
@{
    Layout = "_Layout";
    ViewData["Title"] = "Registrars";
    ViewBag.Active = "Registrar";
}

<div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
    <h1 class="h2">Registrars</h1>
</div>

<div class="card-deck">
    @foreach (var reg in Model)
    {
        <partial name="GroupedZoneSummary" model="@reg.Value"/>
    }
</div>
